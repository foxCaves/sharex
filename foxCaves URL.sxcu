{
  "Version": "13.5.0",
  "Name": "foxCaves URL",
  "DestinationType": "URLShortener",
  "RequestMethod": "POST",
  "RequestURL": "https://foxcaves.doridian.net/api/v1/links",
  "Parameters": {
    "url": "$input$"
  },
  "Headers": {
    "Authorization": "Basic $base64:USERNAME:APIKEY$"
  },
  "URL": "$json:short_url$",
  "DeletionURL": "https://foxcaves.doridian.net/api/v1/links/$json:id$/delete?redirect=1"
}
