{
  "Version": "13.5.0",
  "Name": "foxCaves",
  "DestinationType": "FileUploader",
  "RequestMethod": "POST",
  "RequestURL": "https://foxcaves.doridian.net/api/v1/files",
  "Parameters": {
    "name": "$filename$"
  },
  "Headers": {
    "Authorization": "Basic $base64:USERNAME:APIKEY$"
  },
  "Body": "Binary",
  "URL": "$json:direct_url$",
  "ThumbnailURL": "$json:thumbnail_url$",
  "DeletionURL": "https://foxcaves.doridian.net/api/v1/files/$json:id$/delete?redirect=1"
}
